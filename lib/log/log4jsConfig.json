{
    "appenders": [
        {
            "type": "dateFile",
            "filename": "logs/log.log",
            "pattern": "_yyyy-MM-dd",
            "alwaysIncludePattern": false,
            "category": "dateFileLog"
        },
        {
            "type": "file",
            "filename": "log/app.log",
            "maxLogSize": 10485760,
            "numBackups": 3
        }
    ],
    "replaceConsole": true,
    "levels":{
        "dateFileLog":"INFO"
    }
}